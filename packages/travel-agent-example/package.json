{
  "name": "travel-agent-example",
  "version": "1.5.4",
  "private": true,
  "scripts": {
    "build": "travel-agent build",
    "build:production": "travel-agent build -p",
    "dev": "travel-agent dev",
    "start:production": "travel-agent start",
    "test": "echo 'plz I can haz tests'"
  },
  "dependencies": {
    "@lonelyplanet/travel-agent": "^1.11.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "reflect-metadata": "^0.1.10",
    "typescript": "^2.5.3"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.2",
    "@types/webpack-env": "^1.13.2",
    "jest": "^22.4.3",
    "jest-css-modules": "^1.1.0",
    "supertest": "^3.0.0"
  },
  "jest": {
    "setupFiles": [
      "./test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js",
      "^.+\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testMatch": [
      "**/__tests__/**/*.spec.(ts|tsx)"
    ]
  }
}
